{
  "name": "bitrealm",
  "version": "1.0.0",
  "description": "Browser-native, plugin-ready, open-source MMORPG builder",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "vite",
    "build": "vite build",
    "start": "node server/index.js",
    "test": "jest",
    "docker:build": "docker build -t bitrealm .",
    "docker:run": "docker-compose up"
  },
  "keywords": [
    "mmorpg",
    "game-builder",
    "multiplayer",
    "colyseus",
    "pixijs",
    "browser-game",
    "javascript",
    "vue",
    "open-source"
  ],
  "author": "bitrealm contributors",
  "license": "MIT",
  "dependencies": {
    "@colyseus/core": "^0.15.0",
    "@colyseus/monitor": "^0.15.0",
    "@colyseus/playground": "^0.15.0",
    "@colyseus/ws-transport": "^0.15.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/multipart": "^7.4.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^6.6.1",
    "@trpc/client": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "@vueuse/core": "^10.7.0",
    "adm-zip": "^0.5.16",
    "ajv": "^8.17.1",
    "bcryptjs": "^2.4.3",
    "colyseus.js": "^0.15.0",
    "dotenv": "^16.5.0",
    "fastify": "^4.13.0",
    "helmet": "^6.0.1",
    "howler": "^2.2.3",
    "jsonwebtoken": "^9.0.0",
    "jszip": "^3.10.1",
    "lz-string": "^1.5.0",
    "minio": "^8.0.5",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.8.0",
    "pinia": "^2.1.7",
    "pixi.js": "^8.0.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.5.1",
    "sharp": "^0.32.0",
    "vue": "^3.4.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
