{
  "name": "bitrealm",
  "version": "1.0.0",
  "description": "Browser-native, plugin-ready, open-source MMORPG builder",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon src/server/index.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node src/server/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix"
  },
  "keywords": [
    "mmorpg",
    "game-builder",
    "open-source",
    "browser-game",
    "game-engine",
    "typescript",
    "react"
  ],
  "author": "bitrealm contributors",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "redux": "^4.2.1",
    "react-redux": "^8.0.5",
    "@reduxjs/toolkit": "^1.9.3",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "jszip": "^3.10.1",
    "file-saver": "^2.0.5",
    "fabric": "^5.2.4",
    "pixi.js": "^7.2.4",
    "@types/fabric": "^4.5.11",
    "matter-js": "^0.19.0",
    "howler": "^2.2.3",
    "lz-string": "^1.5.0",
    "nanoid": "^4.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^9.0.1",
    "@types/file-saver": "^2.0.5",
    "@types/matter-js": "^0.18.2",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.29.3",
    "eslint": "^8.36.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.21",
    "concurrently": "^7.6.0"
  }
}